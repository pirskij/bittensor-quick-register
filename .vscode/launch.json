    "version": "0.2.0",
    "configurations": [
        {
            "name": "(GDB) Debug Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/btreg",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo build"
        },
        {
            "name": "(GDB) Debug with args",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/btreg",
            "args": ["subnet-info", "--subnet", "1"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo build"
        },
        {
            "name": "(LLDB) Debug Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/btreg",
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "cargo build"
        }
    ]
}